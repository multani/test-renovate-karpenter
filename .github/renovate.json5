{
  // Where to find repository-specific updates
  "regexManagers": [
    {
      "fileMatch": ["karpenter/Makefile"],
      "matchStrings": ["chart_version = (?<currentValue>.+?)\\n"],
      "depName": "karpenter",
      "datasource": "docker",
      "registryUrls": ["oci://public.ecr.aws/karpenter"],
      "customChangelogUrl": "https://github.com/kubernetes-sigs/karpenter/",
    },
  ],
}

